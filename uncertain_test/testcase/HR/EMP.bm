<?xml version="1.0" encoding="UTF-8"?>
<ns1:model xmlns:ns1="http://www.aurora-framework.org/schema/bm" baseTable="EMP" alias="e">
    <ns1:fields>
        <ns1:field databaseType="BIGINT" datatype="long" insertExpression="EMP_S.nextval" name="empno"/>
        <ns1:field databaseType="VARCHAR" datatype="string" name="ename"/>
        <ns1:field databaseType="BIGINT" datatype="string" name="job"/>
        <ns1:field databaseType="BIGINT" datatype="long" name="mgr"/>
        <ns1:field databaseType="DATE" datatype="date" name="hiredate"/>
        <ns1:field databaseType="BIGINT" datatype="long" name="deptno"/>
        <ns1:field databaseType="FLOAT" datatype="long" name="sal"/>
        <ns1:field databaseType="FLOAT" datatype="long" name="comm"/>
        <ns1:field databaseType="DATE" datatype="date" insertExpression="sysdate" updateExpression="sysdate" name="creation_date"/>
        <ns1:field databaseType="DATE" datatype="date" expression="sysdate" forInsert="false" name="NOW_DATE"/>
        <ns1:ref-field sourceField="dname" relationName="d" name="dname"/>
        <ns1:ref-field sourceField="ename" relationName="MGR" name="mgr_name"/>
    </ns1:fields>
    <ns1:primary-key>
        <ns1:field sequenceName="EMP_S" name="empno"/>
    </ns1:primary-key>
    <ns1:relations>
        <ns1:relation joinType="INNER" refModel="testcase.HR.DEPT" name="d">
            <ns1:reference foreignField="DEPTNO" localField="DEPTNO"/>
        </ns1:relation>
        <ns1:relation joinType="LEFT OUTER" refAlias="M1" refModel="testcase.HR.EMP" name="MGR">
            <ns1:reference foreignField="EMPNO" localField="MGR"/>
            <ns1:reference expression="M1.DEPTNO is not null"/>
        </ns1:relation>
    </ns1:relations>
    <ns1:query-fields>
        <ns1:query-field queryOperator="like" matchMode="full" field="ename"/>
        <ns1:query-field queryOperator="=" field="deptno"/>
        <ns1:query-field queryExpression="exists (select * from emp t1 where t1.mgr=e.mgr)" field="mgr"/>
    </ns1:query-fields>
    <ns1:data-filters>
        <ns1:data-filter enforceActions="query" expression="e.deptno is not null" name="MustInDepartment"/>
        <ns1:data-filter expression="e.creation_date is not null"/>
    </ns1:data-filters>
    <ns1:order-by>
        <ns1:field name="DNAME"/>
        <ns1:field order="desc" name="hiredate"/>
    </ns1:order-by>
</ns1:model>
