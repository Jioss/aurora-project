<ss:schema xmlns:ss="http://www.uncertain-framework.org/schema/simple-schema">
	<ss:attributes>
		<ss:attribute name="Width" type="string" categoryName="BasicControl" />
		<ss:attribute name="Height" type="string"  categoryName="BasicControl" />
	</ss:attributes>
	<ss:categories>
		<ss:category name="Controls" title="Screen Controls" />
		<ss:category name="BasicControl" title="Basic Controls" parentName="Controls"/>
	</ss:categories>
	<ss:elements>
		<ss:element name="ToRef"/>
		<ss:element name="ToRef2"/>
		<ss:element name="parent">
			<ss:attributes>
				<ss:attribute ref="Width" />
				<ss:attribute ref="Height" />
			</ss:attributes>
			<ss:elements>
				<ss:element ref="ToRef2" />
			</ss:elements>
		</ss:element>
		<ss:element name="child">
			<ss:attributes>
				<ss:attribute name="Color" type="string" />
			</ss:attributes>
			<ss:elements>
				<ss:element ref="ToRef" />
			</ss:elements>
			<ss:extensions>
			    <ss:extension base="parent" />
			</ss:extensions>			
		</ss:element>
	</ss:elements>
</ss:schema>
