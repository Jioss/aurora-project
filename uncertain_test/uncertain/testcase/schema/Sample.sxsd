<sc:schema xmlns:sc="http://www.uncertain-framework.org/schema/simple-schema" targetNamespace="http://www.aurora-application.com/schema/">

	<namespaces>
		<sc:namespace prefix="sc" url="http://www.uncertain-framework.org/schema/ocm" />
		<sc:namespace prefix="ar" url="http://www.aurora-application.com/schema/" />
	</namespaces>

	<types>
		<sc:complexType name="mytype1">
			<attributes/>
			<elements/>
			<validators/>
			<collections/>
			<features>
				<feature class="uncertain.proc.Test" />
			</features>
		</sc:complexType>
		<sc:simpleType name="mytype2" base="string">
			<enumerations>
				<sc:enumeration value="AK" prompt="" />
				<sc:enumeration value="BC" prompt="" />
				<sc:enumeration value="DK" prompt="" />
			</enumerations>
		</sc:simpleType>
	</types>
	
	<attributes>
		<sc:attribute name="DataType" type="xs:string" />
		<sc:attribute name="Height" type="xs:string" >
			<validators>
				<sc:rangeValidator maxValue="1000" minValue="100"/>
			</validators>			
		</sc:attribute>
	</attributes>
	
	<elements>
	
		<sc:element name="element" >
			<attributes/>
			<collections>
				<sc:collection name="attributes" minOccur="0" maxOccur="unbound" type="sc:attribute" />
				<sc:collection name="elements" minOccur="0" maxOccur="unbound" type="sc:element" />
			</collections>
			<elements>
				<element ref="ar:collection" />
				<element name="" type="" />				
			</elements>
			<validators/>
			<extensions>
			    <sc:extension base="sc:complexType" />
			</extensions>
		</sc:element>
	
	</elements>
	

	<categories>
		<sc:category name="sc:schema" prompt="Schema definition" />
		<sc:category name="sc:basicType" prompt="Basic data type" parent="sc:schema" />
	</categories>
	
</sc:schema>