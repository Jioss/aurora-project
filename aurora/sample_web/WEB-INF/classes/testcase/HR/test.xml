<sql-service>

    <fields>

        <field name="value"  datatype="java.lang.Long" databaseType="BIGINT" />

        <field name="name" datatype="java.lang.String" databaseType="VARCHAR" />
        
        <field name="workflow_id" />

    </fields>

    <query>

        select '申请者自己' name,

               0 value 

          from dual 

         union all  

        select ename,

               empno  

          from emp

          #WHERE_CLAUSE#

    </query>

    <query-fields>

        <query-field field="workflow_id" queryExpression="e.empno is not null"/>

    </query-fields>

</sql-service>
